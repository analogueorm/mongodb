{
    "name": "analogue/mongodb",
    "description": "MongoDB Driver for Analogue ORM",
    "keywords": ["orm", "datamapper", "laravel", 
    "entity", "mongodb", "mapper"],
    "homepage": "http://github.com/analogueorm/mongodb",
    "license": "MIT",
    "authors": [
        {
            "name": "RÃ©mi Collin",
            "email": "rcollin2@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "analogue/orm": "dev-master",
        "jenssegers/mongodb": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "laravel/laravel": "~5.3",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "analogue/factory": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Analogue\\MongoDB\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/MongoTestCase.php"
        ],
        "psr-4": {
            "Test\\": "tests/domain"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable":true
}