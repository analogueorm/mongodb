{
    "name": "analogue/mongodb",
    "description": "MongoDB Driver for Analogue ORM",
    "keywords": ["orm", "datamapper", "laravel", 
    "entity", "mongodb", "mapper"],
    "homepage": "http://github.com/analogueorm/mongodb",
    "license": "MIT",
    "authors": [
        {
            "name": "RÃ©mi Collin",
            "email": "rcollin2@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "analogue/orm": "dev-master",
        "jenssegers/mongodb": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "laravel/laravel": "~5.4",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "analogue/factory": "~1.1.0",
        "laravel/browser-kit-testing": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Analogue\\MongoDB\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/MongoTestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/domain"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable":true
}